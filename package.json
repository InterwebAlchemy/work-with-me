{
  "name": "work-with-me",
  "private": true,
  "scripts": {
    "build:app": "next build",
    "build:types": "openapi-typescript https://$SUPABASE_PROJECT_ID.supabase.co/rest/v1/?apikey=$SUPABASE_API_KEY --output ./src/types/supabase.ts",
    "dev": "npm run dev:next",
    "dev:next": "next dev",
    "lint:app": "next lint",
    "prepare": "is-ci || prepare:local",
    "prepare:env": "copyfiles ./.env.example ./.env.local",
    "prepare:local": "prepare:env && prepare:supabase",
    "prepare:supabase": "npm run supabase:init",
    "start": "next start",
    "supabase:init": "supabase init",
    "test": "echo 'No tests configured...' && exit 0"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@supabase/supabase-js": "^1.29.4",
    "@supabase/ui": "^0.36.3",
    "cross-fetch": "^3.1.5",
    "framer-motion": "^5.6.0",
    "humps": "^2.0.1",
    "next": "12.0.10",
    "prettier": "^2.5.1",
    "querystring": "^0.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "@types/humps": "^2.0.1",
    "@types/node": "17.0.14",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "copyfiles": "^2.4.1",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "is-ci": "^3.0.1",
    "openapi-typescript": "^5.1.1",
    "typescript": "4.5.5"
  }
}
